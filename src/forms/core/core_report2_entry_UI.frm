    Say = 7
End If

RefSatir = 0
Set WsRaporNo = ThisWorkbook.Worksheets(10)
If Rapor1TarihiText.Value <> "" Then
    StrRaporTarihiGlobal = Right(CStr(Rapor1TarihiText.Value), 4)
