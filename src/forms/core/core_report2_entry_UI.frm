Sub Teknik_AKontrol()
Dim ctl As MSForms.Control, OgeFrame As Integer, Kont As Integer, KontSatir As Integer
Dim Teknik_ASay As Integer, Teknik_AOlmayanSay As Integer

'invalidlerin tümü Technique A ise sadece Technique A seçilemez. Tümü seçilmelidir.
'invalid Technique A yoksa kullanıcının ilerlemesine izin verme.

If SadeceTeknik_AGecersizlerOption.Value = True Or TumuOption.Value = True Or _
   XXXMudPaketiOptionEvet.Value = True Or XXXMudPaketiOptionHayir.Value = True Then
    'Satır sayısını hesapla
    Kont = 0
    KontSatir = 0
    For OgeFrame = 1 To 19
        If Controls("OgeTuruFrame" & OgeFrame).Visible = True Then
            Kont = OgeFrame
            KontSatir = OgeFrame
        End If
    Next OgeFrame
    KontSatir = KontSatir + 1
    
    If KontSatir = 1 Then
    
        'Rapor2_2 kategorisi değil
        If Left(UretimOzelligi.Value, 11) <> "Technique A" Then
            SadeceTeknik_AGecersizlerOption.Value = False
            TumuOption.Value = False
            XXXMudPaketiOptionEvet.Value = False
            XXXMudPaketiOptionHayir.Value = False
            Tutanak2BirlestirmeOptionEvet.Enabled = True
            Tutanak2BirlestirmeOptionHayir.Enabled = True
            Tutanak2BirlestirmeOptionEvet.Value = False
            Tutanak2BirlestirmeOptionHayir.Value = False
            Tutanak2BirlestirmeOptionEvet.Enabled = False
            Tutanak2BirlestirmeOptionHayir.Enabled = False
            MsgBox "Since this operation does not contain an invalid Type A item with the 'Technique A' production technique, it cannot be saved as a Report 2.2 operation. Please uncheck the Report 2.2 box above before proceeding.", vbOKOnly + vbExclamation, "Enterprise Document Automation System"
            GoTo Son
        End If
        If SadeceTeknik_AGecersizlerOption.Value = True Then
            'Rapor2_2 kategorisinde, ama Tümü seçilmeli.
            If Sonuc.Value = "invalid" And Left(UretimOzelligi.Value, 11) = "Technique A" Then
                MsgBox "Since all invalid Type A items in the operation contain the 'Technique A' production technique, the 'Only Technique A' option cannot be selected. Therefore, after closing this message, the 'All' option will be automatically selected.", vbOKOnly + vbExclamation, "Enterprise Document Automation System"
                SadeceTeknik_AGecersizlerOption.Value = False
                TumuOption.Value = True
                GoTo Son
            End If
        End If
    Else 'Kont>0 anlamına gelir.
    
        'Rapor2_2 kategorisi değil
        Teknik_AOlmayanSay = 0
        If Left(UretimOzelligi.Value, 11) <> "Technique A" Then
            Teknik_AOlmayanSay = Teknik_AOlmayanSay + 1
        End If
        For OgeFrame = 1 To Kont
            If Left(Controls("UretimOzelligi" & OgeFrame).Value, 5) <> "Technique A" Then
                Teknik_AOlmayanSay = Teknik_AOlmayanSay + 1
            End If
        Next OgeFrame
        If Teknik_AOlmayanSay = Kont + 1 Then
            SadeceTeknik_AGecersizlerOption.Value = False
            TumuOption.Value = False
            XXXMudPaketiOptionEvet.Value = False
            XXXMudPaketiOptionHayir.Value = False
            Tutanak2BirlestirmeOptionEvet.Enabled = True
            Tutanak2BirlestirmeOptionHayir.Enabled = True
            Tutanak2BirlestirmeOptionEvet.Value = False
            Tutanak2BirlestirmeOptionHayir.Value = False
            Tutanak2BirlestirmeOptionEvet.Enabled = False
            Tutanak2BirlestirmeOptionHayir.Enabled = False
            MsgBox "Since this operation does not contain an invalid Type A item with the 'Technique A' production technique, it cannot be saved as a Report 2.2 operation. Please uncheck the 'Report 2.2' box above before proceeding.", vbOKOnly + vbExclamation, "Enterprise Document Automation System"
            GoTo Son
        End If
    
        If SadeceTeknik_AGecersizlerOption.Value = True Then
            'Rapor2_2 kategorisinde, ama Tümü seçilmeli.
            Teknik_ASay = 0
            If Sonuc.Value = "invalid" And Left(UretimOzelligi.Value, 11) = "Technique A" Then
                Teknik_ASay = Teknik_ASay + 1
            End If
            For OgeFrame = 1 To Kont
                If Controls("Sonuc" & OgeFrame).Value = "invalid" And Left(Controls("UretimOzelligi" & OgeFrame).Value, 11) = "Technique A" Then
                    Teknik_ASay = Teknik_ASay + 1
                End If
            Next OgeFrame
            If Teknik_ASay = Kont + 1 Then
                MsgBox "Since all invalid Type A items in this operation have the 'Technique A' production technique, the 'Only Technique A' option cannot be selected. Therefore, the 'All' option will be automatically selected after closing this message.", vbOKOnly + vbExclamation, "Enterprise Document Automation System"
                SadeceTeknik_AGecersizlerOption.Value = False
                TumuOption.Value = True
                GoTo Son
            End If
        End If
    End If
End If

Son:

End Sub

