Private Sub IlgiliBirim_DblClick(ByVal Cancel As MSForms.ReturnBoolean)

'Me.IlgiliBirim.DropDown
IlgiliBirim.BackColor = RGB(255, 255, 255)
IlgiliBirim.ForeColor = RGB(30, 30, 30)

End Sub

Private Sub IlgiliBirim_KeyDown(ByVal KeyCode As MSForms.ReturnInteger, ByVal Shift As Integer)

    On Error Resume Next
    
   Select Case KeyCode
        Case 38  'Yukarı
            If IlgiliBirim.ListIndex <= 0 Then KeyCode = 0
            Abort = True
            If Not KeyCode = 0 Then
                KeyCode = 0
                IlgiliBirim.ListIndex = IlgiliBirim.ListIndex - 1
            End If
            Me.IlgiliBirim.DropDown
            
        Case 40 'Aşağı
            If IlgiliBirim.ListIndex = IlgiliBirim.ListCount - 1 Then KeyCode = 0
            Abort = True
            If Not KeyCode = 0 Then
                KeyCode = 0
                IlgiliBirim.ListIndex = IlgiliBirim.ListIndex + 1
            End If
            Me.IlgiliBirim.DropDown
    End Select
    Abort = False
    
End Sub

Private Sub IlgiliBirim_Change()

If IlgiliBirim.ListIndex = -1 And IlgiliBirim.Value <> "" Then
   IlgiliBirim.Value = ""
   GoTo Son
End If

If IlgiliBirim.Value <> "" Then
    IlgiliBirim.SelStart = 0
    IlgiliBirim.SelLength = Len(IlgiliBirim.Value)
End If

Son:
IlgiliBirim.DropDown
If IlgiliBirim.BackColor = RGB(60, 100, 180) Then
IlgiliBirim.BackColor = RGB(255, 255, 255)
IlgiliBirim.ForeColor = RGB(30, 30, 30)
End If

End Sub

